<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!--
      Definition des valeurs d'un bean pour les types primitifs et autres elements
      qui ne sont pas des beans
     -->
    <bean class="fr.moon.formation.model.BeanListeDesPrimitifEtAutre">
        <property name="entier" value="20"></property>
        <property name="caractere" value="A"></property>
        <property name="chaine" value="Moustafa"></property>
        <property name="fichier" value="appContext007.xml"></property>
        <property name="localisation" value="fr_FR"></property>
        <property name="properties">
            <value>
                profile=dev
                version=1.2
            </value>
        </property>
        <property name="url" value="https://spring.io"></property>
        <property name="reel" value="100"></property>
        <property name="map">
            <map>
                <entry key="cle1" value="valeur1"></entry>
                <entry key="cle2" value="valeur2"></entry>
            </map>
        </property>
        <property name="list">
            <list>
                <value>valeur1</value>
                <value>valeur2</value>
            </list>
        </property>
        <property name="tableauChaines" value="toto,riri"></property>
        <property name="tableauBytes" value="valeur"></property>
        <property name="set">
            <set>
                <value>Valeur3</value>
                <value>Valeur4</value>
            </set>
        </property>


    </bean>


</beans>